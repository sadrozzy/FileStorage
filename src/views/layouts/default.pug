block variables
- const serviceName = "CloudStorage"

doctype html
html(lang='en')
  head
    meta(charset='utf-8')
    meta(name='viewport' content='width=device-width, initial-scale=1, maximum-scale=1')

    link(rel='icon' href=require('Images/favicons/favicon32.png') type='image/png' sizes='64x64')
    link(rel='icon' href=require('Images/favicons/favicon64.png') type='image/png' sizes='32x32')

    title= title

    block style


    block head
      link(rel='stylesheet' href=require('Styles/main.scss'))
      link(rel='stylesheet' href=require('Styles/_colors.scss'))

  body
    .container
      block content
      block script